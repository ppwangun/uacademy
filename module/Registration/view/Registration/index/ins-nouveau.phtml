                            <label>Nom et Prénoms de la mère:</label>
                            <input name="motherName" ng-model="$ctrl.student.motherName" required />  
                                <div ng-messages="stdRegistrationForm.motherName.$error">
                                    <div ng-message="required">le champ nom est obligatoire</div>

                                </div>   
                          </md-input-container>

                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Profession</label>
                            <input name="motherProfession" ng-model="$ctrl.student.motherProfession" required />
                                <div ng-messages="stdRegistrationForm.motherProfession.$error">
                                    <div ng-message="required">le champ Prénom est obligatoire</div>
                                    <div ng-message="minlength">le code doit avoir au moins 2 caractères</div>
                                </div> 
                          </md-input-container>
                        </div> 
                           <div layout-gt-sm="row" style='margin-top: 5px;' >
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Pays de Résidence</label>
                                    <md-select name="motherCountry" ng-model="$ctrl.student.motherCountry" required >
                                        <md-option ng-repeat="country in $ctrl.countries_1" ng-value="country" >
                                          {{country.name}}
                                        </md-option>
                                      </md-select>
                                    <div ng-messages="stdRegistrationForm.motherCountry.$error">
                                        <div ng-message="required">le champ Ville  de résidence est obligatoire</div>
                                    </div>
                                </md-input-container>
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Ville de résidence</label>
                                    <md-select name="motherCity" ng-model="$ctrl.student.motherCity" md-on-open="$ctrl.getCities_1($ctrl.student.motherCountry.sortname)"  required>
                                    
                                        <md-option ng-repeat="city in $ctrl.cities_1" ng-value="city.id" >
                                          {{city.name}}
                                        </md-option>
                                    </md-select>
                                    <div ng-messages="stdRegistrationForm.motherCity.$error">
                                        <div ng-message="required">le champ Région de résidence est obligatoire</div>
                                    </div>                                    
                                </md-input-container>
                                <md-input-container class="md-block" flex-gt-sm>
                            
                               
                                </md-input-container>

                          </div>
                           <div layout-gt-sm="row" style='margin-top: 15px;' >
                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Numéro de téléphone:</label>
                            <input name="motherPhoneNum" ng-model="$ctrl.student.motherPhoneNum" ng-pattern="/^[0-9]{9}$/" required />
                                <div ng-messages="stdRegistrationForm.motherPhoneNum.$error">
                                    <div ng-message="required">le champ nom est obligatoire</div>
                                    <div ng-message="pattern">Numéro de téléphone invalid</div>

                                </div>   
                          </md-input-container>

                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Email:</label>
                            <input name="motherEmail" type="email" ng-model="$ctrl.student.motherEmail" minlength="10" maxlength="100" ng-pattern="/^.+@.+\..+$/" />
                                <div ng-messages="stdRegistrationForm.motherEmail.$error">
                                       <div ng-message-exp="['required', 'minlength', 'maxlength', 'pattern']">
                                          votre email doit contenir au moins 10 et être semblable à un email.
                                        </div>
                                </div> 
                          </md-input-container>
                        </div> 
 <!-- FIn identification de la mère -->
 
