

<div ng-app="myApp" ng-controller="onlineregCtrl as $ctrl"  ng style="margin-top: 50px; ">
    <md-content id="std-main-content" ng-init="$ctrl.init()">
      <!-- Display workflow bar -->
        <div style="text-align:right; height:10px; margin-bottom: 30px;">
            Draft<img src="img/icons8-forward-50.png" style="height:30px;">
            Attente payment<img src="img/icons8-forward-50.png" style="height:30px;">
            Attente validation<img src="img/icons8-forward-50.png" style="height:30px;">
            Terminer<img src="img/icons8-forward-50.png" style="height:30px;">
        </div>
      <!-- End -->
      <p class="text-warning"><span class='fa fa-warning'>Prière de bien vouloir remplir les différents champs du formulaire. tous les champs suivis de l'astérisk<span class="text-danger">(*) </span> sont obligatoires.
              LE formulaire ne pourra être soumis que si les champs sont bien remplis</span></p>
      <!--Showing tabs -->
        <md-tabs md-dynamic-height md-border-bottom class="md-no-animation padding"  >
            <!--First tab -->
            <md-tab label="Inscription Administrative">
                <md-content >
                <div  layout="column" ng-cloak class="md-inline-form">
                     <form name="stdRegistrationForm">
                    <div class="row">
                        <div class="col-md-3">
                          <img style="width:200px; height:200px;" ng-src="{{imageSrc}}" />      
                          <md-input-container class="md-block" flex-gt-sm>
                            
                            <input type ="file" name="image" ng-file-select="onFileSelect($files)" ng-model="imageSrc"   />
                                <div ng-messages="stdRegistrationForm.image.$error">
                                    <div ng-message="required">le champ photo est obligatoire</div>
                                    
                                </div> 
                          </md-input-container>                            
   
                        </div>
                        <div class="col-md-9">
                            <p class=" ">
                            <span class="text-primary pull-right"><?=$this->studentFaculty ?></span><br />
                            <span class="text-primary pull-right"><?=$this->studentTraining?></span> <br />
                            
                            <span class="text-primary"><?=$this->studentName?></span><span class="pull-right"><?=$this->studentClasse?></span>
                            </p>
                        </div>
                    </div>
                     
                            <md-toolbar>
                            <div class="md-toolbar-tools">
                              <h2>Identifcation de l'étudiant</h2>
                            </div>
                          </md-toolbar>
                          <div layout-gt-sm="row"  style='margin-top: 15px;'>
                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Nom:</label>
                            <input name="name" ng-model="$ctrl.student.name" required />
                                <div ng-messages="stdRegistrationForm.name.$error">
                                    <div ng-message="required">le champ nom est obligatoire</div>

                                </div>   
                          </md-input-container>

                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Prénom:</label>
                            <input name="prenom" ng-model="$ctrl.student.prenom" required />
                                <div ng-messages="stdRegistrationForm.prenom.$error">
                                    <div ng-message="required">le champ Prénom est obligatoire</div>
                                    
                                </div> 
                          </md-input-container>
                        </div>
                        <div layout-gt-sm="row" >
                            <md-input-container class="md-block" flex-gt-sm>
                              <label>Date de naissance</label>
                              <md-datepicker  ng-model="$ctrl.student.birthdate" name="birthdate" required></md-datepicker>

                              <div ng-messages="stdRegistrationForm.birthdate.$error">
                                <div ng-message="valid">Format de date invalide</div>
                                <div ng-message="required">le champ date de naissance est obligatoire</div>

                              </div>
                            </md-input-container>

                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Ville de naissance:</label>
                            <input name="birthplace" ng-model="$ctrl.student.birthplace" required />
                                <div ng-messages="stdRegistrationForm.birthplace.$error">
                                    <div ng-message="required">le champ Prénom est obligatoire</div>
                                    
                                </div> 
                          </md-input-container>
                        </div>
                          <div layout-gt-sm="row"  >
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Sexe</label>
                                    <md-select name='gender' ng-model="$ctrl.student.gender" required >
                                        <md-option value="M">Masculin</md-option>
                                        <md-option value="sword">Féminin</md-option>
                                    </md-select>
                                    <div ng-messages="stdRegistrationForm.gender.$error">
                                        <div ng-message="required">le champ Sexe est obligatoire</div>

                                    </div>                                    
                                </md-input-container>
                          
     
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Numéro de téléphone</label>
                                    <input name="phoneNumber" ng-model="$ctrl.student.phoneNumber" ng-pattern="/^[0-9]{9}$/" required />
                                    <div ng-messages="stdRegistrationForm.phoneNumber.$error">
                                        <div ng-message="required">le champ numéro de téléphone est obligatoire</div>
                                        <div ng-message="pattern">Numéro de téléphone invalid</div>
                         
                                    </div>
                                </md-input-container>
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Email</label>
                                    <input name="email" ng-model="$ctrl.student.email"  type="email" required  minlength="10" maxlength="100" ng-pattern="/^.+@.+\..+$/"  />
                                    <div ng-messages="stdRegistrationForm.email.$error" role="alert">
                                       <div ng-message-exp="['required', 'minlength', 'maxlength', 'pattern']">
                                          votre email doit contenir au moins 10 et être semblable à un email.
                                        </div>
                                    </div>
                                </md-input-container>
   
                        </div>


                          <div layout-gt-sm="row" style='margin-top: 5px;' >
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Langue</label>
                                    <md-select name ='language' ng-model="$ctrl.student.language" required >
                                        <md-option value="FR">Français</md-option>
                                        <md-option value="ANG">Anglais</md-option>
                                      </md-select>
                                    <div ng-messages="stdRegistrationForm.language.$error">
                                        <div ng-message="required">le champ Langue est obligatoire</div>
                                    </div> 
                                </md-input-container>
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Nationalité</label>
                                    <md-select name='nationality' ng-model="$ctrl.student.nationality" required  >
                                        <md-option ng-repeat="country in $ctrl.countries" ng-value="country.name" >
                                          {{country.name}}
                                        </md-option>
                                      </md-select>
                                    <div ng-messages="stdRegistrationForm.nationality.$error">
                                        <div ng-message="required">le champ Nationalité est obligatoire</div>
                                    </div>                                     
                                </md-input-container>
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Réligion</label>
                                    <md-select name='religion' ng-model="$ctrl.student.religion" required >
                                        <md-option value="CATHOLIQUE">Catholique</md-option>
                                        <md-option value="PROTESTANT">Protestant</md-option>
                                        <md-option value="MUSULMAN">Musulman</md-option>
                                        <md-option value="AUTRE">Autres</md-option>
                                      </md-select>
                                    <div ng-messages="stdRegistrationForm.religion.$error">
                                        <div ng-message="required">le champ Religion est obligatoire</div>
                                    </div> 
                                </md-input-container>

                          </div>
                          <div layout-gt-sm="row" style='margin-top: 15px;' >
                          
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Situation matrimoniale</label>
                                    <md-select name='matrimonialStatus' ng-model="$ctrl.student.matrimonialStatus" required >
                                        <md-option value="Célibataire">Célibataire</md-option>
                                        <md-option value="Marié(e)">Marié(e)</md-option>
                                        <md-option value="Divorcé(e)">Divorcé(e)</md-option>
                                      </md-select>
                                    <div ng-messages="stdRegistrationForm.matrimonialStatus.$error">
                                        <div ng-message="required">le champ Situation matrimoniale est obligatoire</div>
                                    </div> 
                                </md-input-container>
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Situation d'emploi</label>
                                    <md-select name='employmentStatus' ng-model="$ctrl.student.employmentStatus" required >
                                        <md-option value="Employé">Employé</md-option>
                                        <md-option value="Sans emploi">Sans emploi</md-option>

                                      </md-select>
                                    <div ng-messages="stdRegistrationForm.employmentStatus.$error">
                                        <div ng-message="required">le champ Situation d'emploi est obligatoire</div>
                                    </div> 
                                </md-input-container>
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Handicap</label>
                                    <md-select name='handicap' ng-model="$ctrl.student.handicap" required >
                                        <md-option value="OUI">Oui</md-option>
                                        <md-option value="NON">Non</md-option>

                                      </md-select>
                                    <div ng-messages="stdRegistrationForm.handicap.$error">
                                        <div ng-message="required">le champ Handicap est obligatoire</div>
                                    </div> 
                                </md-input-container>
                            
                          </div>
                        
                          
 <!-- identification du père -->
                          <md-toolbar>
                            <div class="md-toolbar-tools">
                              <h2>Identifcation de du père</h2>
                            </div>
                          </md-toolbar>
                           <div layout-gt-sm="row" style='margin-top: 15px;'>
                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Nom et Prénoms:</label>
                            <input name="fatherName" ng-model="$ctrl.student.fatherName" required />
                                <div ng-messages="stdRegistrationForm.fatherName.$error">
                                    <div ng-message="required">le champ nom est obligatoire</div>

                                </div>   
                          </md-input-container>

                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Profession</label>
                            <input name="fatherProfession" ng-model="$ctrl.student.fatherProfession" required />
                                <div ng-messages="stdRegistrationForm.fatherProfession.$error">
                                    <div ng-message="required">le champ Profession est obligatoire</div>
                                    <div ng-message="minlength">le code doit avoir au moins 2 caractères</div>
                                </div> 
                          </md-input-container>
                        </div> 
                           <div layout-gt-sm="row" style='margin-top: 5px;' >
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Pays de Résidence</label>
                                    <md-select name='fatherCountry' ng-model="$ctrl.student.fatherCountry" required >
                                        <md-option ng-repeat="country in $ctrl.countries" ng-value="country" >
                                          {{country.name}}
                                        </md-option>
                                    </md-select>
                                    <div ng-messages="stdRegistrationForm.fatherCountry.$error">
                                        <div ng-message="required">le champ Pays de résidence est obligatoire</div>
                                    </div> 
                                </md-input-container>

                                <md-input-container class="md-block" flex-gt-sm>
                                <label>Ville de résidence</label>
                                    <md-select name="fatherCity" ng-model="$ctrl.student.fatherCity" md-on-open="$ctrl.getCities($ctrl.student.fatherCountry.sortname)"  required>
                                    
                                        <md-option ng-repeat="city in $ctrl.cities" ng-value="city" >
                                          {{city.name}}
                                        </md-option>
                                    </md-select>
                                    
                                    <div ng-messages="stdRegistrationForm.fatherCity.$error">
                                        <div ng-message="required">le champ Région de résidence est obligatoire</div>
                                    </div> 
                                </md-input-container>
                                <md-input-container class="md-block" flex-gt-sm>

                                </md-input-container>
                          </div>
                           <div layout-gt-sm="row" style='margin-top: 10px;'>
                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Numéro de téléphone:</label>
                            <input name="fatherPhoneNum" ng-model="$ctrl.student.fatherPhoneNum" ng-pattern="/^[0-9]{9}$/" required />
                                <div ng-messages="stdRegistrationForm.fatherPhoneNum.$error">
                                    <div ng-message="pattern">Numéro de téléphone invalid</div>

                                </div>   
                          </md-input-container>

                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Email:</label>
                            <input name="fatherEmail" type="email" ng-model="$ctrl.student.fatherEmail" minlength="10" maxlength="100" ng-pattern="/^.+@.+\..+$/" />
                                <div ng-messages="stdRegistrationForm.fatherEmail.$error">
                                       <div ng-message-exp="['required', 'minlength', 'maxlength', 'pattern']">
                                          votre email doit contenir au moins 10 et être semblable à un email.
                                        </div>
                                </div> 
                          </md-input-container>
                        </div> 
 <!-- FIn identification du père -->
 
  <!-- identification de la mère -->
                            <md-toolbar>
                            <div class="md-toolbar-tools">
                              <h2>Identifcation de la mère</h2>
                            </div>
                          </md-toolbar>
                             <div layout-gt-sm="row" style='margin-top: 15px;'>
