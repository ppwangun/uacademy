                          <md-toolbar>
                            <div class="md-toolbar-tools">
                              <h2>Identifcation du tuteur</h2>
                            </div>
                          </md-toolbar>
                             <div layout-gt-sm="row" style='margin-top: 15px;' >
                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Nom et Prénoms:</label>
                            <input name="tutorName" ng-model="$ctrl.student.tutorName"  />
                                <div ng-messages="stdRegistrationForm.tutorName.$error">
                                    <div ng-message="required">le champ nom est obligatoire</div>

                                </div>   
                          </md-input-container>

                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Profession</label>
                            <input name="tutorProfession" ng-model="$ctrl.student.tutorProfession" />
                                <div ng-messages="stdRegistrationForm.tutorProfession.$error">
                                    <div ng-message="required">le champ Prénom est obligatoire</div>
                                    <div ng-message="minlength">le code doit avoir au moins 2 caractères</div>
                                </div> 
                          </md-input-container>
                        </div> 
                           <div layout-gt-sm="row" style='margin-top: 5px;' >
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Pays de Résidence</label>
                                    <md-select name="tutorCountry" ng-model="$ctrl.student.tutorCountry">
                                        <md-option ng-repeat="country in $ctrl.countries_2" ng-value="country" >
                                          {{country.name}}
                                        </md-option>
                                      </md-select>
                                    <div ng-messages="stdRegistrationForm.tutorCountry.$error">
                                        <div ng-message="required">le champ Région de résidence est obligatoire</div>
                                    </div>                                     
                                </md-input-container>
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Ville de résidence</label>
                                    <md-select name="tutorCity" ng-model="$ctrl.student.tutorCity" md-on-open="$ctrl.getCities_2($ctrl.student.tutorCountry.sortname)" >
                                    
                                        <md-option ng-repeat="city in $ctrl.cities_2" ng-value="city.id" >
                                          {{city.name}}
                                        </md-option>
                                    </md-select>
                                    <div ng-messages="stdRegistrationForm.tutorCity.$error">
                                        <div ng-message="required">le champ Région de résidence est obligatoire</div>
                                    </div>                                     
                                </md-input-container>
                                <md-input-container class="md-block" flex-gt-sm>
                               
                                </md-input-container>

                          </div>
                           <div layout-gt-sm="row" style='margin-top: 15px;' >
                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Numéro de téléphone:</label>
                            <input name="tutorPhoneNum" ng-model="$ctrl.student.tutorPhoneNum" ng-pattern="/^[0-9]{9}$/" />
                                <div ng-messages="stdRegistrationForm.tutorPhoneNum.$error">
                                    <div ng-message="required">le champ nom est obligatoire</div>
                                    <div ng-message="pattern">Numéro de téléphone invalid</div>

                                </div>   
                          </md-input-container>

                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Email:</label>
                            <input name="tutorEmail" ng-model="$ctrl.student.tutorEmail" minlength="10" maxlength="100" ng-pattern="/^.+@.+\..+$/" />
                                <div ng-messages="stdRegistrationForm.tutorEmail.$error">
                                       <div ng-message-exp="['required', 'minlength', 'maxlength', 'pattern']">
                                          votre email doit contenir au moins 10 et être semblable à un email.
                                        </div>
                                </div> 
                          </md-input-container>
                        </div> 
 <!-- FIn identification du tuteur -->
 
   <!-- information du baccalauréat -->
                               <md-toolbar>
                            <div class="md-toolbar-tools">
                              <h2>Etudes antérieures</h2>
                            </div>
                          </md-toolbar>
                        <div layout-gt-sm="row" style='margin-top: 15px;'>
                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Dernier établissement:</label>
                            <input name="lastSchool" ng-model="$ctrl.student.lastSchool" required />
                                <div ng-messages="stdRegistrationForm.lastSchool.$error">
                                    <div ng-message="required">le champ établissement est obligatoire</div>

                                </div>   
                          </md-input-container>

                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Type du diplôme d'entrée</label>
                                    <md-select name="entranceCertificate" ng-model="$ctrl.student.entranceCertificate" ngchange='$ctrl.isBaccalaureat($ctrl.student.entranceCertificate)' required >
                                        <md-option value="BACC">Baccalauréat</md-option>
                                        <md-option value="BTS">BTS</md-option>
                                        <md-option value="DUT">DUT</md-option>
                                        <md-option value="GCE/A">GCE/A</md-option>
                                        <md-option value="HND">HND</md-option>
                                        <md-option value="LICENCE">Licence</md-option>
                                        <md-option value="MASTER">Master</md-option>
                                      </md-select>
                                    <div ng-messages="stdRegistrationForm.entranceCertificate.$error">
                                        <div ng-message="required">le champ diplôme de résidence est obligatoire</div>
                                    </div>  
                          </md-input-container>
                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Matricule du diplôme:</label>
                            <input name="certificateId" ng-model="$ctrl.student.certificateId" required />
                                <div ng-messages="stdRegistrationForm.certificateId.$error">
                                    <div ng-message="required">le champ matricule est obligatoire</div>

                                </div>   
                          </md-input-container>
                        </div> 
                        <div layout-gt-sm="row" style='margin-top: 5px;' ng-show='$ctrl.isBaccalaureat($ctrl.student.entranceCertificate)'>
                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Série:</label>
                            <md-select name="certificateOption" ng-model="$ctrl.student.certificateOption" ng-required='$ctrl.isBaccalaureat($ctrl.student.entranceCertificate)' >
                                        <md-option value="A">A</md-option>
                                        <md-option value="C">C</md-option>
                                        <md-option value="D">D</md-option>
                                        <md-option value="E">E</md-option>
                                        <md-option value="F1">F1</md-option>
                                        <md-option value="F3">F3</md-option>
                                        <md-option value="F4">F4</md-option>
                                        <md-option value="F5">F5</md-option>
                                        <md-option value="SCIENCE">Science</md-option>
                                        <md-option value="ART">Art</md-option>
                                      </md-select>
                                    <div ng-messages="stdRegistrationForm.entranceCertificate.$error">
                                        <div ng-message="required">le champ Série est obligatoire</div>
                                    </div>   
                          </md-input-container>

                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Centre d'examen:</label>
                            <input name="examCenter" ng-model="$ctrl.student.examCenter" ng-required='$ctrl.isBaccalaureat($ctrl.student.entranceCertificate)' />
                                <div ng-messages="stdRegistrationForm.examCenter.$error">
                                    <div ng-message="required">Ce champ  est obligatoire</div>
                                    <div ng-message="minlength">le code doit avoir au moins 2 caractères</div>
                                </div> 
                          </md-input-container>
                         
                          <md-input-container class="md-block" flex-gt-sm>
                            <label>Session:</label>
                            <input name="examSession" ng-model="$ctrl.student.examSession" ng-required='$ctrl.isBaccalaureat($ctrl.student.entranceCertificate)' />
                                <div ng-messages="stdRegistrationForm.examSession.$error">
                                    <div ng-message="required">Ce champ  est obligatoire</div>

                               </div>   
                          </md-input-container>

